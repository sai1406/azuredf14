{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid27-reporting-adf"
		},
		"ls_ablob_covidreportingasa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingasa'"
		},
		"ls_adls_covidreportingadlsg_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingadlsg'"
		},
		"Is_http_opendata_europe_ecdc_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_adls_covidreportingadlsg_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingadlsg.dfs.core.windows.net/"
		},
		"tr_ingest_population_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/46fcd95b-7159-44a6-8a2b-bac9f1baecaf/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingasa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid27-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingasa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingasa_connectionString')]"
					},
					"ls_adls_covidreportingadlsg_accountKey": {
						"value": "[parameters('ls_adls_covidreportingadlsg_accountKey')]"
					},
					"Is_http_opendata_europe_ecdc_eu_properties_typeProperties_url": {
						"value": "[parameters('Is_http_opendata_europe_ecdc_eu_properties_typeProperties_url')]"
					},
					"ls_adls_covidreportingadlsg_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingadlsg_properties_typeProperties_url')]"
					},
					"tr_ingest_population_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}